    <movies:abstract xml:lang="en-US"><![CDATA[Muse no Kagami  is a 2012 Japanese film directed by Yūichi Fukuda. It is based on the Japanese television drama series of the same name.]]></movies:abstract>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.taotesting.com/movies.rdf#MuseumHours">
    <rdf:type rdf:resource="http://www.taotesting.com/movies.rdf#Movie"/>
